#include <iostream>
using namespace std;

int main() {
    int A[100], B[100];
    int C[2 * 100]; 
    int Na, Nb, Nc;

    Nc = Na + Nb; 

    for (int i = 0; i < Na; ++i) {
        C[i] = A[i];
    }

    for (int i = 0; i < Nb; ++i) {
        C[Na + i] = B[i];
    }
    
    cout << "\nArreglo Concatenado: ";
    for (int i = 0; i < Nc; ++i) {
        cout << C[i] << " ";
    }
    cout << endl;

    return 0;
}
