#include <iostream>
using namespace std;

int main() {
    int A[100], B[100];
    int C[2 * 100]; 
    int Na, Nb; 
    
    int i = 0, j = 0, k = 0;

    while (i < Na && j < Nb) {
        if (A[i] <= B[j]) {
            C[k] = A[i]; 
            i++;         
        } else {
            C[k] = B[j]; 
            j++;         
        }
        k++;
    }

    while (i < Na) {
        C[k] = A[i];
        i++;
        k++;
    }

    while (j < Nb) {
        C[k] = B[j];
        j++;
        k++;
    }

    cout << "\nArreglo Intercalado y Clasificado: ";
    for (int idx = 0; idx < k; ++idx) {
        cout << C[idx] << " ";
    }
    cout << endl;
    return 0;
}
